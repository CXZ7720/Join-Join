      <h1 class="header-w3ls">
         Reservation 
      </h1>
      <!-- multistep form -->
      <div class="main-bothside">
         <form action="step3" method="post">
            <div class="main-title">
               <h2> Select room type / breakfast count </h2>
            </div>
            <div class="form-group">
               <div class="form-mid-w3ls">
                  <select name="room_type"class="form-control buttom">
  
                     <option value="standard1">Standard 1 / 150,000</option>
                     <option value="standard2">Standard 2 / 200,000</option>
                     <option value="executive1">Executive 1 / 300,000</option>
                     <option value="executive2">Executive 2 / 350,000</option>
                     <option value="suite1">Suite 1 / 500,000</option>
                     <option value="suite2">Suite 2 / 550,000</option>
                  </select>
               </div>
            </div>
            <div class="form-group">
               <div class="form-grid-w3ls">
                  <input type="number" name="breakfast_cnt" class="form-control" placeholder="Breakfast count" required="">
               </div>
            </div>
            <input type="hidden" name = check_in value="<%=check_in%>">
            <input type="hidden" name = check_out value="<%=check_out%>">
            <input type="hidden" name = howmany value="<%=howmany%>">
            <input type="submit" value="Next">
         </form>
